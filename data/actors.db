c", "consume": {"type": "", "target": null, "amount": null}, "components": {"concentration": false, "ritual": false, "vocal": true, "somatic": false, "material": false, "value": ""}, "materials": {"value": "", "consumed": false, "cost": 0, "supply": 0}, "preparation": {"mode": "prepared", "prepared": true}, "scaling": {"mode": "none", "formula": ""}}, "sort": 290000, "effects": []}, {"_id": "YTJlMjQ3Mjk1M2Ey", "name": "Power Word Kill", "permission": {"default": 0}, "folder": null, "flags": {}, "type": "spell", "img": "worlds/curse-of-strahd/characters/041_-_Lich/avatar.png", "data": {"description": {"value": "<p>You utter a word of power that can compel one creature you can see within range to die instantly. If the creature you chose has 100 hit points or fewer, it dies. Otherwise, the spell has no effect.</p>", "chat": "", "unidentified": ""}, "source": "", "activation": {"type": "action", "cost": 1, "condition": ""}, "target": {"value": null, "width": null, "units": "", "type": ""}, "range": {"value": 60, "long": null, "units": "ft"}, "duration": {"value": 0, "units": "inst"}, "uses": {"value": 0, "max": 0, "per": ""}, "actionType": "util", "ability": "", "attackBonus": 0, "formula": "", "chatFlavor": "", "critical": {"threshold": null, "damage": null}, "damage": {"parts": [], "versatile": ""}, "save": {"ability": "", "dc": null, "scaling": "spell"}, "level": 9, "school": "enc", "consume": {"type": "", "target": null, "amount": null}, "components": {"concentration": false, "ritual": false, "vocal": true, "somatic": false, "material": false, "value": ""}, "materials": {"value": "", "consumed": false, "cost": 0, "supply": 0}, "preparation": {"mode": "prepared", "prepared": true}, "scaling": {"mode": "none", "formula": ""}}, "sort": 300000, "effects": []}, {"_id": "NGU0OTAzNGQ1MTJl", "name": "Paralyzing Touch", "permission": {"default": 0}, "folder": null, "flags": {}, "type": "weapon", "img": "worlds/curse-of-strahd/characters/041_-_Lich/avatar.png", "data": {"desc